<% form_for [:admin, @cms_block] do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <% f.fields_for :cms_contents do |builder| -%>
    <%= render 'admin/cms_contents/form_fields', :f => builder %>
  <% end -%>
  <p>
    <%= f.submit %>
  </p>
<% end %>
